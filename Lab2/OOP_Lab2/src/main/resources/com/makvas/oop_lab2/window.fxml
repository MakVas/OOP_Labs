<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.makvas.oop_lab2.WindowController">
    <ImageView fitHeight="694.0" fitWidth="1280.0" layoutY="26.0" pickOnBounds="true">
        <Image url="@Content/map.png" />
    </ImageView>
    <MenuBar layoutY="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" onAction="#createAddObjectWindow" text="Add object" />
            <MenuItem mnemonicParsing="false" onAction="#createChangeWindow" text="Edit object" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#createDeleteWindow" text="Delete object" />
        </Menu>
        <Menu mnemonicParsing="false" text="Help" />
        <Menu fx:id="info" mnemonicParsing="false" text="Info">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#createInfoWindow" text="Info" />
         </items></Menu>
    </MenuBar>
</AnchorPane>
